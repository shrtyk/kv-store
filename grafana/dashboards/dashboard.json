{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 10,
      "panels": [],
      "title": "HTTP Performance",
      "type": "row"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Total HTTP requests per second across all endpoints",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 0,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "mode": "none", "group": "A" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 80 }
            ]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{key=~\"($key)\"}[1m])) or on() vector(0)",
          "legendFormat": "Total Requests",
          "refId": "A"
        }
      ],
      "title": "Request Rate",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Top endpoints generating errors",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "align": "left",
            "displayMode": "auto",
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 80 }
            ]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 1 },
      "id": 3,
      "options": {
        "footer": { "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Value" }]
      },
      "pluginVersion": "10.4.1",
      "targets": [
        {
          "expr": "topk(5, sum by (path) (rate(http_requests_total{code=~\"4..|5..\", key=~\"($key)\"}[1m]))) or on() vector(0)",
          "format": "table",
          "instant": true,
          "interval": "",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Top Error Paths",
      "type": "table"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Latency distribution across all HTTP requests",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "continuous-RdYlBu" },
          "custom": {
            "axisLabel": "Latency (s)",
            "axisPlacement": "auto",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "scaleDistribution": { "type": "log" }
          },
          "mappings": []
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 7 },
      "id": 4,
      "options": {
        "color": { "mode": "spectrum", "scheme": "Oranges" },
        "exemplar": { "color": "rgba(255,0,0,0.7)" },
        "legend": { "show": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "expr": "sum by (le) (rate(http_requests_seconds_bucket[1m])) or on() vector(0)",
          "format": "heatmap",
          "legendFormat": "{{le}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Latency Distribution",
      "type": "heatmap"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Current latency percentiles for HTTP requests",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.1 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 7 },
      "id": 5,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "10.4.1",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_requests_seconds_bucket[1m])) by (le)) or on() vector(0)",
          "format": "time_series",
          "instant": true,
          "interval": "",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(http_requests_seconds_bucket[1m])) by (le)) or on() vector(0)",
          "format": "time_series",
          "instant": true,
          "interval": "",
          "legendFormat": "P90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_requests_seconds_bucket[1m])) by (le)) or on() vector(0)",
          "format": "time_series",
          "instant": true,
          "interval": "",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "title": "HTTP Latency Percentiles",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 },
      "id": 20,
      "panels": [],
      "title": "Key-Value Store Operations",
      "type": "row"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Current latency percentiles for each operation type",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.05 },
              { "color": "red", "value": 0.1 }
            ]
          },
          "unit": "s"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "PUT P99" },
            "properties": [
              {
                "id": "color",
                "value": { "fixedColor": "#9940ba", "mode": "fixed" }
              }
            ]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 16 },
      "id": 22,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(puts_latency_seconds_bucket[1m])) by (le)) or on() vector(0)",
          "format": "time_series",
          "instant": true,
          "interval": "",
          "legendFormat": "PUT P99",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(deletes_latency_seconds_bucket[1m])) by (le)) or on() vector(0)",
          "format": "time_series",
          "instant": true,
          "interval": "",
          "legendFormat": "DELETE P99",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(gets_latency_seconds_bucket[1m])) by (le)) or on() vector(0)",
          "format": "time_series",
          "instant": true,
          "interval": "",
          "legendFormat": "GET P99",
          "refId": "C"
        }
      ],
      "title": "P99 Latency by Operation",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 30 },
      "id": 30,
      "panels": [],
      "title": "Error Rates",
      "type": "row"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "description": "Current error rates by HTTP status class",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          },
          "unit": "percent"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "5xx Rate" },
            "properties": [
              {
                "id": "color",
                "value": { "fixedColor": "red", "mode": "fixed" }
              }
            ]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 31 },
      "id": 32,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "10.4.1",
      "targets": [
        {
          "expr": "(sum(rate(http_requests_total{code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m]))) or on() vector(0)",
          "format": "time_series",
          "instant": true,
          "interval": "",
          "legendFormat": "5xx Rate",
          "refId": "A"
        },
        {
          "expr": "(sum(rate(http_requests_total{code=~\"4..\"}[5m])) / sum(rate(http_requests_total[5m]))) or on() vector(0)",
          "format": "time_series",
          "instant": true,
          "interval": "",
          "legendFormat": "4xx Rate",
          "refId": "B"
        }
      ],
      "title": "Error Rates",
      "type": "stat"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["production", "prometheus", "metrics"],
  "templating": {
