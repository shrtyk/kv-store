        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "definition": "label_values(http_requests_total, key)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "key",
        "options": [],
        "query": {
          "query": "label_values(http_requests_total, key)",
